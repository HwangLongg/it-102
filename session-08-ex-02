#include <stdio.h>
int main () {
    int n ;
    int array [5] = {1,2,3,4,5};
    printf ("nhap vao ky tu bat ky");
    scanf ("%d",&n);
    for (int i = 0 ; i < 5 ; i++){
        if ( n == array[i] ){
        printf (" vị trí phần tử trong mảng là %d",array[i]);
        return 0;
    }} printf ("phần tử không tồn tại trong mảng ");
    return 0;
}